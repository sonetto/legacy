<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="RARC Composer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="..\bin\Debug\RARC Composer.exe" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\rundir\" />
				<Option object_output="..\obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="..\bin\Release\RARC Composer.exe" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\rundir\" />
				<Option object_output="..\obj\Release\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="`wx-config --cflags`" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add directory="$(WX_HOME)\include" />
			<Add directory="$(WX_HOME)\contrib\include" />
			<Add directory="..\include" />
			<Add directory="..\dependencies\include" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add library="wx_msw-2.8" />
			<Add library="kernel32" />
			<Add library="user32" />
			<Add library="gdi32" />
			<Add library="comdlg32" />
			<Add library="comctl32" />
			<Add library="advapi32" />
			<Add library="shell32" />
			<Add library="ole32" />
			<Add library="oleaut32" />
			<Add library="rpcrt4" />
			<Add library="uuid" />
			<Add library="wsock32" />
			<Add library="odbc32" />
			<Add library="libwxpng-2.8" />
			<Add library="zdll" />
			<Add directory="$(WX_HOME)\lib" />
			<Add directory="..\dependencies\lib" />
		</Linker>
		<Unit filename="..\include\MainFrame.h" />
		<Unit filename="..\include\MainFrameImpl.h" />
		<Unit filename="..\include\RARCFile.h" />
		<Unit filename="..\include\RARCTreeData.h" />
		<Unit filename="..\include\TreeIterator.h" />
		<Unit filename="..\include\main.h" />
		<Unit filename="..\resource\resource.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="..\src\MainFrame.cpp" />
		<Unit filename="..\src\MainFrameImpl.cpp" />
		<Unit filename="..\src\RARCFile.cpp" />
		<Unit filename="..\src\RARCTreeData.cpp" />
		<Unit filename="..\src\TreeIterator.cpp" />
		<Unit filename="..\src\main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
