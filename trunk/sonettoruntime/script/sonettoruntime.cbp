<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Sonetto_Runtime" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="..\..\bin\release\runtime\game" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\bin\release\runtime\" />
				<Option object_output="..\obj\release\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="libsonetto" />
				</Linker>
			</Target>
			<Target title="Debug">
				<Option output="..\..\bin\debug\runtime\game" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\bin\debug\runtime\" />
				<Option object_output="..\obj\rebug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add library="libsonetto_d" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="Release;Debug;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-fident" />
			<Add option="-pipe" />
			<Add option="-DWIN32" />
			<Add option="-D_WINDOWS" />
			<Add directory="..\" />
			<Add directory="..\include" />
			<Add directory="..\src" />
			<Add directory="..\..\libsonetto\include" />
			<Add directory="..\..\dependencies\include" />
			<Add directory="$(OGRE_HOME)\OgreMain\include" />
		</Compiler>
		<Linker>
			<Add directory="..\..\lib" />
		</Linker>
		<Unit filename="..\main.cpp" />
		<Unit filename="..\resource\resource.rc">
			<Option compilerVar="WINDRES" />
			<Option target="Release" />
		</Unit>
		<Unit filename="..\resource\resource_d.rc">
			<Option compilerVar="WINDRES" />
			<Option target="Debug" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger>
				<remote_debugging target="Release">
					<options conn_type="0" serial_port="" serial_baud="115200" ip_address="" ip_port="" additional_cmds="" additional_cmds_before="" skip_ld_path="0" />
				</remote_debugging>
				<remote_debugging target="Debug">
					<options conn_type="0" serial_port="" serial_baud="115200" ip_address="" ip_port="" additional_cmds="" additional_cmds_before="" skip_ld_path="0" />
				</remote_debugging>
				<remote_debugging target="Debug">
					<options conn_type="0" serial_port="" serial_baud="115200" ip_address="" ip_port="" additional_cmds="" additional_cmds_before="" skip_ld_path="0" />
				</remote_debugging>
				<remote_debugging target="Release">
					<options conn_type="0" serial_port="" serial_baud="115200" ip_address="" ip_port="" additional_cmds="" additional_cmds_before="" skip_ld_path="0" />
				</remote_debugging>
				<remote_debugging target="Release">
					<options conn_type="0" serial_port="" serial_baud="115200" ip_address="" ip_port="" additional_cmds="" additional_cmds_before="" skip_ld_path="0" />
				</remote_debugging>
				<remote_debugging target="Debug">
					<options conn_type="0" serial_port="" serial_baud="115200" ip_address="" ip_port="" additional_cmds="" additional_cmds_before="" skip_ld_path="0" />
				</remote_debugging>
			</debugger>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
