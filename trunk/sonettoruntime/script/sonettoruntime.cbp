<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Sonetto_Runtime" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="..\..\bin\release\runtime\game.exe" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\bin\release\runtime\" />
				<Option object_output="..\obj\release\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="libsonetto" />
				</Linker>
			</Target>
			<Target title="Debug">
				<Option output="..\..\bin\debug\runtime\game.exe" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\bin\debug\runtime\" />
				<Option object_output="..\obj\rebug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add library="libsonetto_d" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="Release;Debug;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-fident" />
			<Add option="-pipe" />
			<Add option="-D_STFU_VORBIS" />
			<Add option="-DWIN32" />
			<Add option="-D_WINDOWS" />
			<Add directory="..\include" />
			<Add directory="..\..\libsonetto\include" />
			<Add directory="..\..\dependencies\include" />
			<Add directory="$(OGRE_HOME)\OgreMain\include" />
		</Compiler>
		<Linker>
			<Add directory="..\..\lib" />
		</Linker>
		<Unit filename="..\resource\resource.rc">
			<Option compilerVar="WINDRES" />
			<Option target="Release" />
		</Unit>
		<Unit filename="..\resource\resource_d.rc">
			<Option compilerVar="WINDRES" />
			<Option target="Debug" />
		</Unit>
		<Unit filename="..\src\main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
